package section4;

public class Circle {
    public int radius;
	
	public Circle( int r ){
		super("Circle"); 
		radius = r;
	}
	public double computerArea(){ // 추상메소드를 구현
		return (double)width*height;
	}
	public double computerPerimeter(){  // 추상메소드를 구현
		return 2.0 * Math.PI *radius;
	}
	public String toString()
	{
		return "Rectangle: width is " + width + ",height is " + height;
	}
}
