package Section1;

public class Code01 {

	public static void main(String[] args) {

		int x;
		x = 100;
		
		double y = 1.0023;

		Person1 first; //클래스 선언이지만 하나의 타입처럼 선언할 수 있다
		first = new Person1(); //배열처럼 생성해야함 객체 생성
		
		
		first.name =  "john"; 
	    first.number = "01246323"; //Person1 타입의 first라는 변수에 이름과 전화번호를 저장하는 것
	}

}
