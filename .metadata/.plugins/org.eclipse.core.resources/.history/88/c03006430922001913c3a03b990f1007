package section4;

public class Circle extends Shape {
    public int radius;
	
	public Circle( int r ){
		super("Circle"); 
		radius = r;
	}
	public double computerArea(){ // 추상메소드를 구현
		return Math.PI * radius * radius;
	}
	public double computerPerimeter(){  // 추상메소드를 구현
		return 2.0 * Math.PI * radius;
	}
	public String toString()
	{
		return "Circle: radius is " + radius;
	}
}
