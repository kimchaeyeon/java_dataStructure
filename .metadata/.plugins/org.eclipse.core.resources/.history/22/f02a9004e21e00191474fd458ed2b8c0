		while(true){
			System.out.print("$ ");
			String command = kb.next();
			if ( command.equals("addevent")){
				
			}
			else if( command.equals("list")){
				
			}
			else if( command.equals("show")){
				
			}
			else if( command.equals("exit")){
				break;
			}
		}
		kb.close();
	}
	
