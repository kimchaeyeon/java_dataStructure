package section2;
//하나의 점을 표현하기 위해서
public class MyRectangle2 {
	public MyPoint2 lu;//왼쪽 위쪽
	public int width;
	public int height;  

    public MyRectangle2(int x, int y, int w, int h){
    	lu = new MyPoint2(x,y);
    	width = w;
    	height = h;
    }
    public MyRectangle2(MyPoint2 p, int w, int h){
    	lu = p;
    	width = w;
    	height = h;
    }

}

